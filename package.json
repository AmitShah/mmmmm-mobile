{
  "name": "MMMMM",
  "version": "0.0.5-alpha",
  "private": true,
  "scripts": {
    "lib": "tsc",
    "prepare": "patch-package",
    "propagate-replacements": "propagate-replacement-fields --field=react-native",
    "start": "npm run lib && npm run propagate-replacements && node node_modules/react-native/local-cli/cli.js start",
    "test-e2e-android": "cd android/app && ./test-features.sh || cd ../..",
    "test": "jest",
    "update-android-version": "react-native-version --never-amend --target android && git add -A && git commit -m \"Release $(cat package.json | $(npm bin)/jase version)\"",
    "android-release": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "move-android-apk": "mkdir -p gitignored/release && cp android/app/build/outputs/apk/app-release.apk gitignored/release/",
    "release": "npm run update-android-version && npm run android-release && npm run move-android-apk"
  },
  "dependencies": {
    "@cycle/isolate": "3.2",
    "@cycle/run": "3.4",
    "@types/react": "15.0.38",
    "@types/react-native": "0.46.4",
    "@types/react-native-vector-icons": "^4.2.0",
    "assert": "^1.4.1",
    "cycle-native-navigation": "1.2.0",
    "cycle-onionify": "4.0.0",
    "depject": "^4.1.0",
    "depnest": "^1.3.0",
    "human-time": "0.0.1",
    "mutant": "3.21.2",
    "patchcore": "1.12.1",
    "prop-types": "^15.5.10",
    "pull-stream": "3.6.0",
    "react": "16.0.0-alpha.12",
    "react-native": "0.48.3",
    "react-native-navigation": "^1.1.219",
    "react-native-scuttlebot": "10.4.14",
    "react-native-simple-markdown": "^1.0.60-rc.3",
    "react-native-ssb-client": "2.0.3",
    "react-native-vector-icons": "4.2.0",
    "rn-viewpager": "^1.2.3",
    "ssb-ref": "2.7.1",
    "xstream": "11.x",
    "xstream-from-mutant": "1.1",
    "xstream-from-pull-stream": "1.1"
  },
  "devDependencies": {
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "2.1.0",
    "cheerio": "^1.0.0-rc.2",
    "jase": "1.2.0",
    "jest": "20.0.4",
    "patch-package": "^3.4.0",
    "prettier": "1.5.x",
    "propagate-replacement-fields": "1.2.0",
    "react-native-version": "^2.3.1",
    "react-test-renderer": "16.0.0-alpha.12",
    "tslint": "^5.7.0",
    "typescript": "2.4.x",
    "yarn": "^1.0.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "react-native": {
    "ssb-client": "react-native-ssb-client"
  }
}
