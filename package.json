{
  "name": "MMMMM",
  "version": "0.0.7-alpha",
  "private": true,
  "scripts": {
    "lib": "tsc",
    "prepare": "patch-package",
    "propagate-replacements": "propagate-replacement-fields --field=react-native",
    "start": "npm run lib && npm run propagate-replacements && node node_modules/react-native/local-cli/cli.js start",
    "test-e2e-android": "cd android/app && ./test-features.sh",
    "test": "jest",
    "update-android-version": "react-native-version --never-amend --target android",
    "clean-up-bundler": "watchman watch-del-all && rm -rf $TMPDIR/react-*",
    "android-release": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "move-android-apk": "mkdir -p gitignored/release && cp android/app/build/outputs/apk/app-release.apk gitignored/release/",
    "commit-after-release": "git add -A && git commit -m \"Release $(cat package.json | $(npm bin)/jase version)\" && git tag v$(cat package.json | $(npm bin)/jase version)",
    "release": "npm run update-android-version && npm run clean-up-bundler && npm run lib && npm run propagate-replacements && npm run android-release && npm run test-e2e-android && npm run move-android-apk && npm run commit-after-release"
  },
  "dependencies": {
    "@cycle/isolate": "3.2",
    "@cycle/run": "3.4",
    "@types/react": "15.0.38",
    "@types/react-native": "0.49.5",
    "@types/react-native-vector-icons": "4.2.0",
    "assert": "^1.4.1",
    "cycle-native-navigation": "1.4.0",
    "cycle-onionify": "4.0.0",
    "depject": "^4.1.0",
    "depnest": "^1.3.0",
    "human-time": "0.0.1",
    "mutant": "3.21.2",
    "patchcore": "1.15.1",
    "prop-types": "15.5.x",
    "pull-stream": "3.6.0",
    "react": "16.0.0-beta.5",
    "react-native": "0.49.5",
    "react-native-dialogs": "0.0.20",
    "react-native-navigation": "1.1.256",
    "react-native-scuttlebot": "10.4.15-rc.7",
    "react-native-simple-markdown": "^1.0.60-rc.3",
    "react-native-ssb-client": "3.0.0-rc.1",
    "react-native-ssb-client-keys": "1.0.0",
    "react-native-ssb-shims": "1.0.2",
    "react-native-vector-icons": "4.2.0",
    "react-native-os": "git://github.com/staltz/react-native-os.git#staltz-package",
    "@staltz/rn-viewpager": "1.2.5",
    "ssb-ref": "2.7.1",
    "xstream": "11.x",
    "xstream-from-callback": "1.0",
    "xstream-from-mutant": "1.1",
    "xstream-from-pull-stream": "1.1"
  },
  "devDependencies": {
    "babel-jest": "20.0.3",
    "babel-preset-react-native": "2.1.0",
    "cheerio": "^1.0.0-rc.2",
    "jase": "1.2.0",
    "jest": "20.0.4",
    "patch-package": "^3.4.0",
    "prettier": "1.5.x",
    "propagate-replacement-fields": "1.2.0",
    "react-native-version": "^2.3.1",
    "react-test-renderer": "16.0.0-beta.5",
    "tslint": "^5.7.0",
    "typescript": "2.4.x",
    "yarn": "^1.0.1"
  },
  "jest": {
    "preset": "react-native"
  },
  "react-native": {
    "os": "react-native-os",
    "rn-viewpager": "@staltz/rn-viewpager",
    "ssb-client": "react-native-ssb-client"
  }
}
